<center>
<h1>Players</h1>

<table>
  <thead>
    <tr>
      <th>UIN</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Address</th>
      <th>Person Type</th>
      <th>Dues</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= player.uin %></td>
        <td><%= player.name %></td>
        <td><%= player.email %></td>
        <td><%= player.phone_number %></td>
        <td><%= player.address %></td>
        <td><%= player.person_type %></td>
        <td><%= player.dues %></td>
        <td><%= player.status %></td>
        <td><%= link_to 'Show', player %></td>
        <td><%= link_to 'Edit', edit_player_path(player) %></td>
        <td><%= link_to 'Delete', delete_player_path(player) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</center>


